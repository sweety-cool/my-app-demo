// Database
datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// Generators
generator client {
 provider = "prisma-client-js"
}

generator docs {
  provider = "node node_modules/prisma-docs-generator"
}


model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  wishlist  String?
}


